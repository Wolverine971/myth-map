{
	"buildCommand": "pnpm run build",
	"devCommand": "pnpm run dev",
	"installCommand": "pnpm install",
	"framework": "sveltekit",
	"regions": ["iad1"],
	"functions": {
		"src/routes/api/**/*.ts": {
			"runtime": "nodejs20.x",
			"memory": 1024,
			"maxDuration": 10
		},
		"src/routes/**/+page.server.ts": {
			"runtime": "nodejs20.x",
			"memory": 512,
			"maxDuration": 10
		},
		"src/routes/**/+layout.server.ts": {
			"runtime": "nodejs20.x",
			"memory": 512,
			"maxDuration": 10
		}
	},
	"crons": [],
	"headers": [
		{
			"source": "/api/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "s-maxage=60, stale-while-revalidate=300"
				}
			]
		},
		{
			"source": "/map/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/(.*).png",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/(.*).jpg",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	],
	"rewrites": [
		{
			"source": "/sitemap.xml",
			"destination": "/api/sitemap"
		}
	]
}